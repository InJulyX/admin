        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.mapper.BotConfigMapper">
    <insert id="insert" parameterType="BotConfig">
        INSERT INTO bot_config
        <trim prefix="(" suffix=")" suffixOverrides=",">
